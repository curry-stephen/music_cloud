<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yy.dao.List_songMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yy.pojo.List_song">
        <id column="id" property="id" />
        <result column="song_id" property="songId" />
        <result column="song_list_id" property="songListId" />
    </resultMap>
<sql id="Base_Column_List">
    id,song_id,song_list_id
</sql>
    
    <select id="allListSong" resultMap="BaseResultMap">
        select  * from tp_music.list_song
    </select>
    
    <select id="selectByPrimaryKey" resultMap="BaseResultMap">
        select 
        <include refid="Base_Column_List"/>
            from tp_music.list_song
            where id=#{id}
    </select>
    <select id="listSongOfSongId" resultMap="BaseResultMap" parameterType="java.lang.Integer">
        select * from tp_music.list_song
        where song_list_id=#{songListId}
    </select>
    <delete id="deleteListSong" parameterType="java.lang.Integer">
        delete from tp_music.list_song
        where song_id=#{songId}
    </delete>
    
    <insert id="insertSelective" parameterType="com.yy.pojo.List_song">
        insert into list_song
        <trim prefix="(" suffix=")" suffixOverrides="," >
            <if test="id != null" >
                id,
            </if>
            <if test="songId != null" >
                song_id,
            </if>
            <if test="songListId != null" >
                song_list_id,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides="," >
            <if test="id != null" >
                #{id,jdbcType=INTEGER},
            </if>
            <if test="songId != null" >
                #{songId,jdbcType=INTEGER},
            </if>
            <if test="songListId != null" >
                #{songListId,jdbcType=INTEGER},
            </if>
        </trim>
    </insert>
    
    <update id="updateListSongMsg" parameterType="com.yy.pojo.List_song">
        update list_song
        <set >
            <if test="songId != null" >
                song_id = #{songId,jdbcType=INTEGER},
            </if>
            <if test="songListId != null" >
                song_list_id = #{songListId,jdbcType=INTEGER},
            </if>
        </set>
        where id = #{id,jdbcType=INTEGER}
    </update>
    <update id="updateByPrimaryKey" parameterType="com.yy.pojo.List_song">
        update tp_music.list_song
        set song_id=#{songId},
        song_list_id=#{songListId}
        where id=#{id}
    </update>
    <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
        delete from tp_music.list_song
        where id=#{id}
    </delete>
</mapper>
